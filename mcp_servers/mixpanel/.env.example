# Get these from your Mixpanel project settings
MIXPANEL_PROJECT_TOKEN=your_project_token_here
MIXPANEL_API_SECRET=your_api_secret_here

# For service account authentication (recommended for server-side)
MIXPANEL_SERVICE_ACCOUNT_USERNAME=your_service_account_username
MIXPANEL_SERVICE_ACCOUNT_SECRET=your_service_account_secret

# Port for the MCP server to listen on
MIXPANEL_MCP_SERVER_PORT=5000